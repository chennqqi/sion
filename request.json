[
    {
        "location": ".*",
        "allowed_method": [
            "GET"
        ],
        "rules": [
            {
                ":target": "GET",
                "id": "[0-9]+"
            }
        ]
    },
    {
        "location": "^/",
        "allowed_method": [
            "GET"
        ]
    },
    {
        "location": "^/hoge",
        "allowed_method": [
            "POST", "GET"
        ],
        "rules": [
            {
                ":target": "POST",
                ":handle_to": "/403page",
                "id": "^[0-9]*$",
                "password": "^[a-zA-Z0-9]*$"
            },
            {
                ":target": "GET",
                ":response_code": "403",
                "id": "^[0-9]*$"
            },
            {
                ":target": "GET",
                "hoge": "^[a-z]+$",
                "@hoge": "fuga"
            }
        ]
    },
    {
        "location": "^/fuga/(.*)/piyo$",
        "allowed_method": [
            "GET",
            "POST"
        ],
        "rules": [
            {
                "[target]": "REGEX",
                "1": "[a-zA-Z0-9\\-]+"
            }
        ]
    }
]
