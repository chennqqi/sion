[
    {
        "location": ".*",
        "allowed-method": [
            "GET"
        ],
        "rules": [
            {
                "[target]": "GET",
                "[if-filtered]": "/error",
                "id": "[0-9]+"
            }
        ]
    },
    {
        "location": "^/",
        "allowed-method": [
            "GET"
        ]
    },
    {
        "location": "^/hoge",
        "allowed-method": [
            "POST", "GET"
        ],
        "rules": [
            {
                "[target]": "POST",
                "[if-filtered]": "/403",
                "id": "[0-9]+",
                "password": "[a-zA-Z0-9]+"
            },
            {
                "[target]": "GET"
            }
        ]
    },
    {
        "location": "^/fuga/(.*)/piyo$",
        "allowed-method": [
            "GET",
            "POST"
        ],
        "rules": [
            {
                "[target]": "REGEX",
                "$1": "[a-zA-Z0-9\\-]+"
            }
        ]
    }
]
